{
  "name": "freecad_webdemo_clientcode",
  "description": "WebDemo for FreeCad / Client Side JS / TS / Web Code",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "npm run build-all:dev",
    "build-all:dev": "npm run build-vendor:dev && npm run build-site:dev",
    "build-site:dev": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress",
    "build-vendor:dev": "cross-env NODE_ENV=development webpack --config webpack.config.vendor.js --progress",
    "build-all:prod": "npm run build-vendor:prod && npm run build-site:prod",
    "build-site:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --hide-modules",
    "build-vendor:prod": "cross-env NODE_ENV=production webpack --config webpack.config.vendor.js --progress --hide-modules",
    "lint": "eslint -c ./.eslintrc.js src/**/*.js  src/**/*.vue  src/**/*.json webpack*.js",
    "install": "pnpm run build-all:dev"
  },
  "dependencies": {
    "@mdi/font": "^5.3.45",
    "@quasar/extras": "^1.8.2",
    "ajv": "^6.12.2",
    "event-source-polyfill": "^1.0.15",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.5.1",
    "quasar": "^1.12.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-router": "^3.3.4",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.4.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.38",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "aspnet-webpack": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.2.0",
    "extract-css-chunks-webpack-plugin": "^4.7.5",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.0",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass": "^1.26.8",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.2",
    "vue-property-decorator": "^9.0.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
